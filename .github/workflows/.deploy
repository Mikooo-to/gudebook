name: AWS S3 bucket deployment
on:
  push:
    branches:
      - master
    paths:
      - "frontend/**"

jobs:
  build:
    runs-on: ubuntu-latest
    container:
      image: node:20-alpine    

    steps:
      - name: Checkout
        uses: actions/checkout@v4

      - name: Build
        run: |
          npm ci
          npm run build
          npm run generate


